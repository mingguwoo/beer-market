<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nio.ngfs.plm.bom.configuration.infrastructure.repository.mapper.BomsConfigurationRuleMapper">

    <select id="getMaxRuleNumber" resultType="java.lang.String">
        select rule_number
        from boms_configuration_rule
        order by rule_number desc
        limit 1
    </select>

</mapper>
